<% unless @xls %> <%= link_to image_tag("xls.png"), vaccinations_report_organization_path(format: "xls", as_of_date: @as_of_date) %> <% end %>

<table id="animal_vaccination_table"  >
  <thead class="fixedHeader">
	<tr>
	  <th></th>
	  <th>name</th>
	  <th>vaccination</th>
	  <th>date due</th>
	</tr>
  </thead>
  <tbody class="scrollContent">
    <% @animal_vaccinations.each do |vaccination| %>
      
	  <tr id="<%= vaccination.animal.id %>">
	  	<td><%= thumbnail(vaccination.animal, '50x50', 'th' , 'Organization') %></td>
	  	<td><%= vaccination.animal.name %></td>
	  	<td><%= vaccination.vaccination.name %></td>
	  	<td><%= vaccination.vaccination_due %></td>
	  </tr>
	<% end %>
  </tbody>
</table>
