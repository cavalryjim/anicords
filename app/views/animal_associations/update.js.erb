$(".reveal-modal").foundation('reveal', 'close');
$("tr#<%=@animal_association.animal_id%>").effect("highlight", {color:"#81F781"}, 1500).unbind();
<% if @animal_association.checked_in %>
  $("tr#<%=@animal_association.animal_id%>").addClass("current_client");
<% else %>
  $("tr#<%=@animal_association.animal_id%>").remove();
<% end %>
