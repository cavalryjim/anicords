$("form#organization_picture_upload").on("ajax:success", function(event, data, status, response) {
  $(this).effect("highlight", {}, 1500);
  $("#org_animal_picture_div").html('<%= j render("organizations/animal_pictures") %>')
}).on("ajax:error", function(event, response, error) {
  return alert("There was a problem.  Please try again later.");
});