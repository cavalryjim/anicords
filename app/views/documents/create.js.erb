<% if @success %>
  $("form#animal_document_upload").effect("highlight", {color:"#81F781"}, 1500).unbind();
  $("#animal_document_div").html('<%= j render("animals/document_fieldsets") %>')
  $("form#animal_document_upload")[0].reset();
<% else %>
  return alert("There was a problem.  Please try again later.");
<% end %>
